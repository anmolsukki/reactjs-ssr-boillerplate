{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-console": "off",
    "no-unused-vars": "off"
  },
  "plugins": ["prettier"]
}
